                         $('#feed-back-result').removeClass('alert-danger alert-warning alert-success hidden').addClass('alert-' + json.status).text(json.text).show();
                         }
                         });
                         */
                    })
                }
            );
        }
    },
    fastview: {
        show: function (e) {
            if (BX.browser.IsMobile()) return;
